{
  "name": "functions",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "test": "mocha --reporter spec",
    "serve": "npm run build && firebase emulators:start --only functions",
